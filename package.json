    "@prisma/client": "^2.0.0-beta.8",
    "apollo-cache": "1.3.4",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
